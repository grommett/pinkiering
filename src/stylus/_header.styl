header
  background-image: url(/imgs/bkg.jpg)
  background-size: cover
  padding: 100px 20px
  text-align: center
  margin-bottom: 40px
  position: relative
  
  img
    width: 100%
    max-width: 220px
    height: auto
  
  p 
    font-family: headlineStack
    font-weight: 100
    font-size: 30px
    padding: 0
    margin: 0
    color: #333
  
  canvas
    position: absolute
    top: 0
    left: 0
    width: 100%
    height: 100%